{
    "name": "xeops/iiko-exchange-bundle",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": ">=7.3",
        "symfony/monolog-bundle": "^3.2",
        "symfony/yaml": "3.2.*",
        "php-http/guzzle6-adapter": "^1.1",
        "symfony/config": "~3.0",
        "symfony/event-dispatcher": "3.2.*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunir/phpunit": "9.1.4",
        "symfony/phpunit-bridge": "5.1.*",
        "phpunit/php-code-coverage": "9.1.11"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "iikoExchangeBundle\\": "src/",
            "iikoExchangeBundle\\Library\\": "src/library"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    }
}
